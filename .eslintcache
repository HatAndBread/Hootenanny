evel;\n          break;\n        default:\n          return;\n      }\n    });\n  }\n}\n","/Users/joshuahume/projects/noise-rooms/src/MusicLogic/handleKeyboard.js",[],"/Users/joshuahume/projects/noise-rooms/src/Components/Nav/DropDownMenu.js",[],"/Users/joshuahume/projects/noise-rooms/src/MusicLogic/handleNoise.js",[],["186","187"],"/Users/joshuahume/projects/noise-rooms/src/MusicLogic/handleDrone.js",[],"/Users/joshuahume/projects/noise-rooms/src/Components/Instruments/SettingsLink.js",[],"/Users/joshuahume/projects/noise-rooms/src/Components/Instruments/Noise.js",[],["188","189"],"/Users/joshuahume/projects/noise-rooms/src/MusicLogic/Noise.js",[],"/Users/joshuahume/projects/noise-rooms/src/Components/Instruments/Percussion/RhythmMachine.js",[],"/Users/joshuahume/projects/noise-rooms/src/Components/Instruments/Drone/DroneFader.js",[],"/Users/joshuahume/projects/noise-rooms/src/Components/Instruments/Drone/Chords.js",[],"/Users/joshuahume/projects/noise-rooms/src/Pages/Instruments.js",[],"/Users/joshuahume/projects/noise-rooms/src/Components/Join/Keypad.js",[],"/Users/joshuahume/projects/noise-rooms/src/Components/Instruments/Percussion/RhythmMachineDrumSelect.js",[],"/Users/joshuahume/projects/noise-rooms/src/Components/Nav/Nav.js",[],"/Users/joshuahume/projects/noise-rooms/src/Components/Settings/SettingsWaves.js",[],"/Users/joshuahume/projects/noise-rooms/src/MusicLogic/Drone.js",[],"/Users/joshuahume/projects/noise-rooms/src/Components/Instruments/Keyboard/KeyboardKey.js",[],{"ruleId":"190","replacedBy":"191"},{"ruleId":"192","replacedBy":"193"},{"ruleId":"190","replacedBy":"194"},{"ruleId":"192","replacedBy":"195"},{"ruleId":"190","replacedBy":"196"},{"ruleId":"192","replacedBy":"197"},{"ruleId":"190","replacedBy":"198"},{"ruleId":"192","replacedBy":"199"},{"ruleId":"200","severity":1,"message":"201","line":6,"column":25,"nodeType":"202","messageId":"203","endLine":6,"endColumn":32},{"ruleId":"190","replacedBy":"204"},{"ruleId":"192","replacedBy":"205"},{"ruleI